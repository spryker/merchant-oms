<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="MerchantOrder">
    </transfer>

    <transfer name="MerchantOrderItem">
        <property name="idStateMachineItemState" type="int"/>
        <property name="idMerchantOrderItem" type="int"/>
    </transfer>

    <transfer name="MerchantOmsProcess">
        <property name="idMerchantOmsProcess" type="int"/>
        <property name="processName" type="string"/>
    </transfer>

    <transfer name="MerchantOmsProcessCriteriaFilter">
        <property name="merchantReference" type="string"/>
    </transfer>

    <transfer name="MerchantOrderItemCollection">
        <property name="merchantOrderItems" type="MerchantOrderItem[]" singular="merchantOrderItem"/>
    </transfer>

    <transfer name="MerchantOrderResponse">
        <property name="isSuccessful" type="bool"/>
    </transfer>

    <transfer name="MerchantOrderItemResponse">
        <property name="isSuccessful" type="bool"/>
    </transfer>

    <transfer name="MerchantOmsEvent">
        <property name="eventName" type="string"/>
    </transfer>

</transfers>
